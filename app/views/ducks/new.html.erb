<h1> New Duck </h1>

<% if flash[:my_errors] %>
    <%= flash[:my_errors].each do |error| %>
      <p> <%= error %> <p>
    <% end %>
<%  end %>

<%= form_for @duck do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name%> <br><br>

    <%= f.label :description%>
    <%= f.text_field :description%> <br><br>

    <%= f.label :student_id %>
    <%= f.collection_select :student_id, Student.all, :id, :name %>

    <%= f.submit %>

<% end %>