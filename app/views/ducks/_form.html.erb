<%= form_for(@duck) do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.label :description %>
        <%= f.text_field :description %>
    </p>

    <p>
        <%= f.collection_select :student_id, Student.all, :id, :name %>
    </p>
    <%= f.submit %>
<% end %>