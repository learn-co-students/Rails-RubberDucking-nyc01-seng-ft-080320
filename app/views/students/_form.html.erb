
<% if flash[:my_errors] %>
    <%= flash[:my_errors].each do |error| %>
      <p> <%= error %> <p>
    <% end %>
<%  end %>

<%= form_for @student do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name%> <br>

    <%= f.label :mod %>
    <%= f.number_field :mod %><br>

    <%= f.submit %>
    
<% end %>